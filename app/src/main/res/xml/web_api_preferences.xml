<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
<PreferenceCategory android:title="@string/web_server_header">
    <Preference android:title="Scan Config Code" app:key="configcode"/>

    <EditTextPreference
        app:key="url"
        app:title="@string/url_title"
        app:useSimpleSummaryProvider="true" />

    <EditTextPreference
        app:key="port"
        app:title="@string/port_title"
        app:useSimpleSummaryProvider="true"/>
    <SwitchPreferenceCompat
        app:key="https_toggle"
        app:title="@string/https_toggle_title" />
    <Preference app:key="endpoint_update"
        app:title="Update Endpoints"/>

</PreferenceCategory>
    <PreferenceCategory>
        <Preference
            android:title="View endpoints"
            app:key="endpoints"/>
    </PreferenceCategory>
</PreferenceScreen>